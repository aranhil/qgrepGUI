<controlzEx:WindowChromeWindow x:Class="qgrepControls.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:qgrepControls"
        xmlns:toolwindows="clr-namespace:qgrepControls.SearchWindow;assembly=qgrepControls"
        xmlns:controlzEx="urn:controlzex"
        x:Name="Window"
        Width="800"
        Height="600"
        MinWidth="80"
        MinHeight="60"
        FlowDirection="LeftToRight"
        Style="{DynamicResource MyWindowStyle}"
        WindowStartupLocation="CenterScreen"
        WindowState="Normal"
        ShowMinButton="False"
        ShowMaxRestoreButton="False"
        ResizeMode="CanResizeWithGrip"
        SizeToContent="Manual"
        SnapsToDevicePixels="True"
        Topmost="False"
        mc:Ignorable="d" KeyDown="Window_PreviewKeyDown" Closed="Window_Closed">
    <controlzEx:WindowChromeWindow.Resources>
        <ResourceDictionary Source="/qgrepControls;component/ResourceDictionary.xaml" />
    </controlzEx:WindowChromeWindow.Resources>
    <Grid x:Name="WindowContent">
    </Grid>
</controlzEx:WindowChromeWindow>
